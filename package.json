{
  "name": "logo-maker-plugin",
  "description": "A simple editor for creating logos - with support for WordPress.",
  "version": "1.12.0",
  "scripts": {
    "start": "snowpack dev --polyfill-node",
    "start-plugin": "wp-scripts start --hot",
    "plugin-build": "wp-scripts build",
    "linux:make-zip": "npm run plugin-build && zip -r logo_maker_plugin.zip index.php plugin_build && mv logo_maker_plugin.zip development/",
    "font:all": "npm run font:config && npm run font:download",
    "font:config": "node development/createFontConfigFile.js",
    "font:download": "node development/downloadFonts.js",
    "packages-update": "wp-scripts packages-update",
    "gen-docs": "npx typedoc",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@beyonk/google-fonts-webpack-plugin": "^1.5.0",
    "@loadable/component": "^5.14.1",
    "@svgdotjs/svg.js": "^3.0.16",
    "body-scroll-lock": "^3.1.5",
    "classnames": "^2.3.1",
    "jszip": "^3.6.0",
    "opentype.js": "^1.3.3",
    "postcss-import": "^14.0.1",
    "pullstate": "^1.22.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@snowpack/plugin-build-script": "^2.1.0",
    "@snowpack/plugin-dotenv": "^2.1.0",
    "@snowpack/plugin-react-refresh": "^2.4.2",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/css-font-loading-module": "^0.0.7",
    "@types/loadable__component": "^5.13.3",
    "@types/opentype.js": "^1.3.1",
    "@types/react": "^17.0.3",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.14",
    "@types/snowpack-env": "^2.3.3",
    "@types/uuid": "^8.3.0",
    "@types/wordpress__blocks": "^6.4.12",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@wordpress/babel-preset-default": "^6.5.0",
    "@wordpress/blocks": "^6.14.1",
    "@wordpress/i18n": "^4.3.0",
    "@wordpress/scripts": "^20.0.2",
    "autoprefixer": "^10.2.5",
    "css-loader": "^5.2.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "snowpack": "^3.8.8",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.2.3"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 5%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
